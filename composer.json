{
	"name": "nglasl/silverstripe-extensible-search",
	"description": "This module allows user customisation and developer extension of a search page instance, including analytics and suggestions.",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"search",
		"full-text"
	],
	"homepage": "https://github.com/nglasl/silverstripe-extensible-search",
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Nathan Glasl",
		"email": "nathan@symbiote.com.au"
	}],
	"require": {
		"silverstripe/cms": "^4.0 || ^5.0",
		"silverstripe/framework": "^4.0 || ^5.0"
	},
	"autoload": {
		"psr-4": {
			"nglasl\\extensible\\": [
				"src/controllers/",
				"src/extensions/",
				"src/objects/",
				"src/pages/",
				"src/services/",
				"src/tasks/"
			],
			"nglasl\\extensible\\tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "4.1.x-dev"
		},
		"expose": [
			"client"
		]
	}
}
